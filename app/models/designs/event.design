language: "javascript"
views: {
  by_account_id: {
    map: "function(document) {
      if (document['model_class'] == 'Event') {
        emit([document.account_id, document.created_at], null);
      }
    }",
    reduce: "function(key, values, rereduce) {
      if (rereduce) {
        return sum(values);
      } else {
        return values.length;
      }
    }"
  }
}
