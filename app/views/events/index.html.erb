<h1>Listing events</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Message</th>
    <th>Created at</th>
    <th>Application</th>
    <th>Environment</th>
    <th>Version</th>
    <th>Controller</th>
    <th>Action</th>
    <th>Request url</th>
    <th>Request params</th>
    <th>Request data type</th>
    <th>Request data</th>
    <th>Session</th>
    <th>Session data type</th>
    <th>Session data</th>
    <th>Additional data type</th>
    <th>Additional data</th>
    <th>Backtrace</th>
    <th>Node</th>
    <th>Pid</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @events.each do |event| %>
  <tr>
    <td><%= event.title %></td>
    <td><%= event.message %></td>
    <td><%= event.created_at %></td>
    <td><%= event.application %></td>
    <td><%= event.environment %></td>
    <td><%= event.version %></td>
    <td><%= event.controller %></td>
    <td><%= event.action %></td>
    <td><%= event.request_url %></td>
    <td><%= event.request_params %></td>
    <td><%= event.request_data_type %></td>
    <td><%= event.request_data %></td>
    <td><%= event.session_id %></td>
    <td><%= event.session_data_type %></td>
    <td><%= event.session_data %></td>
    <td><%= event.additional_data_type %></td>
    <td><%= event.additional_data %></td>
    <td><%= event.backtrace %></td>
    <td><%= event.node %></td>
    <td><%= event.pid %></td>
    <td><%= link_to 'Show', event %></td>
    <td><%= link_to 'Edit', edit_event_path(event) %></td>
    <td><%= link_to 'Destroy', event, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

